# Adapty Configuration
EXPO_PUBLIC_ADAPTY_API_KEY=your_adapty_api_key_here

# Firebase Configuration (if using Firebase for storage)
EXPO_PUBLIC_FIREBASE_API_KEY=your_firebase_api_key_here
